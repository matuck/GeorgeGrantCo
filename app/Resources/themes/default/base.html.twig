<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="keywords" content="{{ render(controller('matuckGeorgeGrantCoBundle:Default:Keywords')) }}">
    <title>{% block title %}George Grant Company{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <!-- Bootstrap Core CSS -->
    <link href="/assets/vendor/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    {% block stylesheets %}{% endblock %}
    <link href="/assets/themes/default/style.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="/assets/vendor/jquery/dist/jquery.min.js"></script>
</head>

<body>
<div class="wrap">
    <div class="container-fluid">
        <div class="row header-bg-grad">
            <div class="col-sm-12 hidden-xs text-center">
                <a href="/" class=" center-block"><img src="/assets/images/georgegrantheader.gif" /></a>
            </div>
        </div>
    </div>
    <!-- Navigation -->
    <nav class="navbar navbar-inverse" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand visible-xs" href="/">George Grant Co</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse">
                {{ knp_menu_render('main') }}
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- Page Content -->
    <div class="container">
        <div class="row">
            <div class="col-xs-9 col-centered ">
                {% block fos_user_content %}
                {% endblock fos_user_content %}
                {% block body %}
                {% endblock %}
            </div>
        </div>
    </div>
    <!-- /.container -->
</div>
<div class="footer">
    <div class="col-xs-9 col-centered">
        {{ render(controller('matuckGeorgeGrantCoBundle:Default:Footer')) }}
    </div>
</div>
    <!-- Bootstrap Core JavaScript -->
    <!-- jQuery Version 1.11.1 -->
    <script src="/assets/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    {% block javascripts %}{% endblock %}
</body>

</html>
